{
  "manifest_version": 3,
  "name": "Website Tracker",
  "version": "1.0",
  "description": "Tracks time spent on websites and provides productivity analytics.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
